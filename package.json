{
  "name": "codeandconspire.com",
  "version": "2.0.0",
  "description": "Website for code and conspire",
  "main": "index.js",
  "scripts": {
    "test": "standard && dependency-check package.json",
    "start": "node server.js"
  },
  "documentify": {
    "transform": [
      [
        "./lib/document.js",
        {
          "order": "end"
        }
      ]
    ]
  },
  "now": {
    "env": {
      "NODE_ENV": "production",
      "CLOUDFLARE_CODEANDCONSPIRE_ZONE": "@cloudflare-codeandconspire-zone",
      "CLOUDFLARE_CODEANDCONSPIRE_EMAIL": "@cloudflare-codeandconspire-email",
      "CLOUDFLARE_CODEANDCONSPIRE_KEY": "@cloudflare-codeandconspire-key"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeandconspire/codeandconspire.com.git"
  },
  "author": "code and conspire",
  "license": "CC-BY",
  "bugs": {
    "url": "https://github.com/codeandconspire/codeandconspire.com/issues"
  },
  "homepage": "https://github.com/codeandconspire/codeandconspire.com#readme",
  "dependencies": {
    "choo": "^6.13.0",
    "dedent": "^0.7.0",
    "dotenv": "^6.0.0",
    "hstream": "^1.2.0",
    "jalla": "^0.6.1",
    "koa-route": "^3.2.0",
    "nanolru": "^1.0.0",
    "normalize.css": "^8.0.0"
  },
  "devDependencies": {
    "choo-devtools": "^2.5.1",
    "choo-service-worker": "^2.4.0",
    "dependency-check": "^3.2.0",
    "standard": "^11.0.1"
  }
}
